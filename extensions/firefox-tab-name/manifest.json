{
    "manifest_version": 2,
    "name": "Preceptor",
    "version": "1.0",
    "description": "Sends active tab info to local AI app securely",
    "permissions":[
        "tabs",
        "activeTab",
        "<all_urls>"
    ],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_title": "Preceptor"
    }
}

